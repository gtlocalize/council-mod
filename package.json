{
  "name": "council-mod",
  "type": "module",
  "version": "0.1.0",
  "description": "Tiered LLM content moderation with council consensus for edge cases",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "mocha --require tsx test/**/*.test.ts",
    "test:golden": "mocha --require tsx test/golden-set.test.ts",
    "test:library": "tsx scripts/test-library.ts",
    "test:policy-impact": "tsx scripts/test-policy-impact.ts",
    "prettier": "prettier --write .",
    "cli": "tsx src/cli.ts",
    "analyze": "tsx src/cli.ts",
    "generate:tests": "tsx scripts/generate-test-cases.ts",
    "generate:nuanced": "tsx scripts/generate-nuanced-tests.ts",
    "generate:framework": "tsx scripts/generate-framework-tests.ts",
    "generate:context-supplement": "tsx scripts/generate-context-supplement.ts",
    "audit": "tsx scripts/audit-cli.ts",
    "audit:llm": "tsx scripts/llm-audit.ts",
    "audit:claude": "tsx scripts/claude-audit.ts",
    "audit:gpt5": "tsx scripts/gpt5-audit.ts",
    "agreement": "tsx scripts/calculate-agreement.ts",
    "generate:regression": "tsx scripts/generate-tests.ts",
    "dashboard": "tsx scripts/dashboard.ts"
  },
  "contributors": [
    "Jacob Habib <jacob@utilityfueled.com> (https://github.com/jahabeebs)",
    "GTLocalize (https://github.com/gtlocalize)"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "content",
    "moderation",
    "ai",
    "llm",
    "council",
    "hate-speech",
    "multilingual",
    "openai",
    "claude",
    "gemini",
    "profanity",
    "filter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gtlocalize/council-mod"
  },
  "files": [
    "dist",
    "src"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "eventsource-parser": "1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.6",
    "@types/jest": "^29.5.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.12.7",
    "chai": "^4.3.8",
    "dotenv": "^17.2.3",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2"
  }
}
